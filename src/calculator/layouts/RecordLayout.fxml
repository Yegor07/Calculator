<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane prefHeight="0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox fx:id="hBox">
            <children>
                <Label fx:id="textExpression" text="Label"/>

                <Button fx:id="use" mnemonicParsing="false" text="Use"/>
                <Button fx:id="copy" mnemonicParsing="false" text="Copy"/>
                <Button fx:id="delete" mnemonicParsing="false" text="Delete"/>
            </children>
        </HBox>

    </children>
</AnchorPane>
