<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="380.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/15.0.1"
            style="-fx-background-color: white;"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.view_controller.MenuViewController">
    <children>
        <Button fx:id="calculatorMenuButton" layoutX="131.0" layoutY="124.0" mnemonicParsing="false"
                onAction="#clickCalculatorMenuButton" prefHeight="75.0" prefWidth="164.0" text="Calculator"
                AnchorPane.topAnchor="124.0">
            <font>
                <Font size="25.0"/>
            </font>
        </Button>
        <Button fx:id="historyMenuButton" layoutX="131.0" layoutY="230.0" mnemonicParsing="false"
                onAction="#clickHistoryMenuButton" prefHeight="77.0" prefWidth="164.0" text="History"
                AnchorPane.rightAnchor="130.0">
            <font>
                <Font size="25.0"/>
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="114.0" layoutY="14.0" prefHeight="25.0" prefWidth="198.0" text="MENU"
               AnchorPane.topAnchor="14.0">
            <font>
                <Font size="66.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
